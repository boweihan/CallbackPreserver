{
  "name": "callbackpreserver",
  "version": "0.0.2",
  "description": "Reusable function contexts",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boweihan/CallbackPreserver.git"
  },
  "keywords": [],
  "author": "Bowei Han",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boweihan/CallbackPreserver/issues"
  },
  "homepage": "https://github.com/boweihan/CallbackPreserver#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "babel-loader": "^8.0.5",
    "jest": "^24.0.0",
    "prettier": "^1.16.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "files": ["lib/**/*"],
  "dependencies": {
    "@types/lodash": "^4.14.120",
    "lodash": "^4.17.11"
  }
}
